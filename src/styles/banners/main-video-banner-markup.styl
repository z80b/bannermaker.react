.main-video-banner$hash
    position relative
    display block
    width 621px
    height 481px
    float left
    background 50% 0 no-repeat
    background-size 100% auto
    
    &__wrapper
        clear both
    
    &__frame
        display block
        width 100%
        height 100%
        border none
    
    @media screen and (min-width: 1246px)
        width 778px
        height 602px